div#register-not-confirm-holder {
  background-color: rgb(228, 228, 228);
  width: 100%;
  height: 100%;
  text-align: center;
}

div#register-not-confirm-holder p {
  margin: 0;
}

div#register-not-confirm-holder td {
  padding: 10px;
}

div#register-not-confirm-holder div#register-not-confirm-block {
  width: 440px;
  display: inline-block;
}

div#register-not-confirm-holder table#register-not-confirm-block-middle img {
  width: 50px;
}

div#register-not-confirm-holder table#register-not-confirm-block-top img {
  width: 170px;
}

div#register-not-confirm-holder button {
  background-color: rgb(0, 153, 255);
  padding: 10px;
}

div#register-not-confirm-holder div#register-not-confirm-block table {
