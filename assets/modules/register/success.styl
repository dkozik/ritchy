
div#register-success-holder {
  background-color: rgb(228, 228, 228);
  width: 100%;
  height: 100%;
  text-align: center;
}

div#register-success-holder p {
  margin: 0;
}

div#register-success-holder td {
  padding: 10px;
}

div#register-success-holder div#register-success-block {
  width: 440px;
  display: inline-block;
}

div#register-success-holder table#register-success-block-middle img {
  width: 50px;
}

div#register-success-holder table#register-success-block-top img {
  width: 170px;
}

div#register-success-holder button {
  background-color: rgb(51, 204, 102);
  padding: 10px;
}

div#register-success-holder div#register-success-block table {
  width: 100%;
}

div#register-success-holder div#register-success-block table#register-success-block-middle td {
  text-align: center;
}